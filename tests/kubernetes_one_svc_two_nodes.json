{
  "services": [
    {
      "virtualServer": {
        "backend": {
          "nodePort": 30008,
          "nodes": [
            "172.16.0.5",
            "172.16.0.6"
          ],
          "serviceName": "foo",
          "servicePort": 80,
          "healthMonitors": [ {
            "interval": 30,
            "timeout": 20,
            "send": "GET /",
            "protocol": "tcp"
            },
            {
            "interval": 10,
            "timeout": 5,
            "send": "GET /",
            "protocol": "http"
            }
          ]
        },
        "frontend": {
          "balance": "round-robin",
          "mode": "http",
          "partition": "velcro",
          "virtualAddress": {
            "bindAddr": "10.128.10.240",
            "port": 5051
          },
          "sslProfile": {
            "f5ProfileName": "Common/clientssl"
          }
        }
      }
    }
  ]
}
